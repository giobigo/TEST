
@{
    ViewBag.Title = "Index";
}

<h2>გაიარეთ რეგისტრაცია</h2>
<form class="form-group" style="margin-top:50px;">
    
    <input  class="form-control" style="margin-top:5px;"name="Username" type="text" placeholder="მომხმარებლის სახელი" />
        
    
        <input class="form-control" style="margin-top:5px;" name="Email" type="email" placeholder="ელ-ფოსტა" />
    
    
        <input class="form-control" style="margin-top:5px;" name="Password" type="password" placeholder="პაროლი" />
   
          
    <input class="form-control" style="margin-top:5px;" name="Firstname" type="text" placeholder="სახელი" />
    
    
        <input class="form-control" style="margin-top:5px;" name="Lastname" type="text" placeholder="გვარი" />
    
    <button class="btn btn-success" style="margin-top:5px;" type="submit">რეგისტრაცია</button>
</form>

